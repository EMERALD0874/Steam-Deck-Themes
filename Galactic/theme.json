{
  "name": "Galactic",
  "description": "THIS THEME IS NO LONGER SUPPORTED AND MAY BREAK. This theme is intended to be a rounded, clean, and modern look while still providing eye candy when looking at the recent games. This was my first theme pack and my first attempt at learning a variety of new CSS changes such as fonts and backgrounds that work on the go.",
  "author": "EMERALD#0874",
  "target": "System-Wide",
  "version": "v1.9",
  "manifest_version": 2,
  "inject": {
    "background.css": ["SP"]
  },
  "patches": {
    "Installation": {
      "default": "All",
      "type": "dropdown",
      "values": {
        "All": {
          "center-recents.css": ["SP"],
          "colors.css": ["SP", "MainMenu", "QuickAccess"],
          "fonts.css": ["SP", "MainMenu", "QuickAccess"],
          "round.css": ["SP"],
          "round-outline.css": ["SP"]
        },
        "Only colors and fonts": {
          "colors.css": ["SP", "MainMenu", "QuickAccess"],
          "fonts.css": ["SP", "MainMenu", "QuickAccess"]
        },
        "Only background": {}
      }
    },
    "Offline Mode": {
      "default": "No",
      "type": "checkbox",
      "values": {
        "Yes": {
          "offline.css": ["SP", "MainMenu", "QuickAccess"]
        },
        "No": {}
      }
    }
  }
}
